{
  "env": {
    "commonjs": true,
    "jasmine": true,
    "node": true,
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
    "no-restricted-syntax": "off",
    "no-loop-func": "off",
    "no-param-reassign": "off"
  }
}
